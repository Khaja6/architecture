# 2 stage pipeline in python
counter=1
a=int(input("ENTER NUMBER-1-"))
counter=counter+1
b=int(input("ENTER NUMBER-2-"))
counter=counter+1
print("1-ADDITION 2-SUBTRACTION")
print("Enter Your Choice")
choice=int(input())
if choice==1:
    print("Performing Addition...",a+b)
    res=a+b
counter=counter+1
if choice==2:
    print("Performing Subtraction...",a-b)
    res=a-b
    counter=counter+1
if choice>=3:
    print("Enter Correct Input")
    print(res)
    counter=counter+1
print("CYCLE VALUE IS:",counter)
ins=int(input("Enter the No.of instructions:"))
performance_measure =ins/counter
print("performance measure is:", performance_measure)
